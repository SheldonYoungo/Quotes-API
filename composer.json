{
    "name": "sheldonyoungo/quotes-api-package",
    "description": "A Laravel package for retrieving quotes from an external API.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "QuotesApiPackage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.0|^9.0|^12.0",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^7.53",
        "pestphp/pest": "^1.23",
        "pestphp/pest-plugin-laravel": "^1.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "QuotesApiPackage\\Providers\\QuoteServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
